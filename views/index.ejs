<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    
    <!--====== Title ======-->
    <title><%= site.tagline %> | <%= site.name %></title>
    
    <!--====== Meta Description ======-->
    <meta name="description" content="<%= site.description %>">
    
    <!--====== Meta Keywords ======-->
    <meta name="keywords" content="<%= site.keywords %>">
    
    <!--====== Meta Author ======-->
    <meta name="author" content="<%= site.author %>">
    
    <!--====== Meta Robots ======-->
    <meta name="robots" content="index, follow">
    
    <!--====== Viewport ======-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <!--====== Canonical URL ======-->
    <link rel="canonical" href="<%= site.url %>">
    
    <!--====== Open Graph Meta Tags (Facebook) ======-->
    <meta property="og:type" content="website">
    <meta property="og:title" content="<%= site.tagline %>">
    <meta property="og:description" content="<%= site.description %>">
    <meta property="og:url" content="<%= site.url %>">
    <meta property="og:site_name" content="<%= site.name %>">
    <meta property="og:image" content="<%= site.url + site.ogImage %>">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    <meta property="og:locale" content="vi_VN">
    
    <!--====== Twitter Card Meta Tags ======-->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= site.tagline %>">
    <meta name="twitter:description" content="<%= site.description %>">
    <meta name="twitter:image" content="<%= site.url + site.ogImage %>">
    
    <!--====== Schema.org Structured Data (JSON-LD) ======-->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "LocalBusiness",
      "name": "<%= site.name %>",
      "description": "<%= site.description %>",
      "url": "<%= site.url %>",
      "logo": "<%= site.url + site.logo %>",
      "image": "<%= site.url + site.ogImage %>",
      "priceRange": "<%= site.priceRange %>",
      "telephone": "<%= site.phone %>",
      "address": {
        "@type": "PostalAddress",
        "addressCountry": "<%= site.address.country %>",
        "addressLocality": "<%= site.address.city %>"
      },
      "sameAs": [
        "<%= site.socialMedia.facebook %>",
        "<%= site.socialMedia.instagram %>"
      ]
    }
    </script>
    
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Product",
      "name": "Khăn Cotton Thêu Họa Tiết Dân Gian",
      "description": "Khăn cotton cao cấp với họa tiết thêu dân gian Việt Nam",
      "brand": {
        "@type": "Brand",
        "name": "<%= site.name %>"
      },
      "offers": {
        "@type": "AggregateOffer",
        "lowPrice": "35000",
        "highPrice": "260000",
        "priceCurrency": "VND"
      }
    }
    </script>

    <!--====== Favicon Icon ======-->
    <link rel="shortcut icon" href="<%= site.logo %>" type="image/png">
        
    <!--====== Animate CSS ======-->
    <link rel="stylesheet" href="/assets/css/animate.css">
        
    <!--====== Slick CSS ======-->
    <link rel="stylesheet" href="/assets/css/slick.css">
        
    <!--====== Line Icons CSS ======-->
    <link rel="stylesheet" href="/assets/css/LineIcons.css">
        
    <!--====== Bootstrap CSS ======-->
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css">
    
    <!--====== Default CSS ======-->
    <link rel="stylesheet" href="/assets/css/default.css">
    
    <!--====== Style CSS ======-->
    <link rel="stylesheet" href="/assets/css/style.css">
    
    <!--====== Lightbox CSS ======-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.11.4/css/lightbox.min.css">
    
    <!--====== Lightbox Custom CSS ======-->
    <link rel="stylesheet" href="/assets/css/lightbox-custom.css">
    
</head>

<body>
    <!--[if IE]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
  <![endif]-->

    <!--====== PRELOADER PART START ======-->
    <div class="preloader">
        <div class="loader">
            <div class="ytp-spinner">
                <div class="ytp-spinner-container">
                    <div class="ytp-spinner-rotator">
                        <div class="ytp-spinner-left">
                            <div class="ytp-spinner-circle"></div>
                        </div>
                        <div class="ytp-spinner-right">
                            <div class="ytp-spinner-circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== PRELOADER PART ENDS ======-->

    <!--====== HEADER PART START ======-->
    <section class="header_area">
        <div class="header_navbar">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand" href="/">
                                <img src="<%= site.logo %>" alt="Logo" style="height: 40px;"> 
                                <h4 style="color: #1a237e; margin-left: 10px;" class="title d-inline"><%= site.name %></h4>
                            </a>
                            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                            </button>

                            <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                                <ul id="nav" class="navbar-nav ml-auto">
                                    <% navigation.forEach((nav, index) => { %>
                                    <li class="nav-item <%= index === 0 ? 'active' : '' %>">
                                        <a class="page-scroll" href="<%= nav.href %>"><%= nav.name %></a>
                                    </li>
                                    <% }); %>
                                </ul>
                            </div> <!-- navbar collapse -->
                        </nav> <!-- navbar -->
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- header navbar -->

        <div id="home" class="header_slider slider-active">
            <% heroSlides.forEach((slide) => { %>
            <div class="single_slider bg_cover d-flex align-items-center" style="background-image: url(<%= slide.backgroundImage %>)">
                <div class="container">
                    <div class="row">
                        <div class="col-md-9">
                            <div class="slider_content">
                                <h2 class="slider_title <%= slide.id > 1 ? 'wow fadeInUp' : '' %>"><%= slide.title %></h2>
                                <p class="wow fadeInUp"><%= slide.description %></p>
                                <a href="<%= slide.buttonLink %>" class="main-btn"><%= slide.buttonText %></a>
                            </div> <!-- slider content -->
                        </div>
                    </div> <!-- row -->
                </div> <!-- container -->
            </div> <!-- single slider -->
            <% }); %>
        </div> <!-- header slider -->
    </section>
    <!--====== HEADER PART ENDS ======-->
    
    <!--====== COFFEE MENU PART START ======-->
    <section id="menu" class="coffee_menu pt-120 pb-130 bg_cover" style="background-color: #f0f0f0;">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="section_title text-center pb-30">
                        <h4 class="title">Bộ Sưu Tập Nổi Bật</h4>
                        <span class="line">
                            <span class="box"></span>
                        </span>
                        <p class="mt-3" style="color: #000;">Tinh Hoa Nghệ Thuật Việt Trên Chất Liệu Cotton Cao Cấp</p>
                    </div> <!-- section title -->
                </div>
            </div> <!-- row -->
            <div class="row">
                <% products.forEach((productLine) => { %>
                <div class="col-lg-6">
                    <div class="coffee_menu_wrapper">
                        <h5 class="mb-4" style="color: #000; text-align: center;"><%= productLine.name %></h5>
                        <% productLine.products.forEach((product, index) => { %>
                        <div class="single_coffee_menu mt-30 d-flex align-items-center wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="<%= 0.2 + (index * 0.3) %>s">
                            <div class="coffee_menu_image">
                                <a href="<%= product.image %>" data-lightbox="products" data-title="<%= product.name %> - <%= product.description %>">
                                    <img w="131px" h="131px" src="<%= product.image %>" alt="<%= product.alt %>" style="cursor: pointer;">
                                </a>
                            </div>
                            <div class="coffee_menu_content media-body">
                                <h4 class="coffee_name"><%= product.name %><span class="price"><%= product.price %></span></h4>
                                <p><%= product.description %></p>
                            </div>
                        </div>  <!-- single coffee menu -->
                        <% }); %>
                    </div>  <!-- coffee menu wrapper -->
                </div>
                <% }); %>
            </div> <!-- row -->
            <div class="row justify-content-center mt-50">
                <div class="col-lg-6 text-center">
                    <a href="#coffee" class="main-btn">Tìm hiểu thêm</a>
                </div>
            </div>
        </div> <!-- container -->
    </section>
    <!--====== COFFEE MENU PART ENDS ======-->
    
    <!--====== COFFEE PART START ======-->
    <section id="coffee" class="coffee_area pt-120 pb-100">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section_title text-center pb-30">
                        <h4 class="title">Vì Sao Chọn Việt Chỉ?</h4>
                        <span class="line">
                            <span class="box"></span>
                        </span>
                    </div> <!-- section title -->
                </div>
            </div> <!-- row -->
            <div class="row justify-content-center">
                <% features.forEach((feature, index) => { %>
                <div class="col-lg-3 col-md-6 col-sm-9">
                    <div class="single_coffee text-center mt-30 wow fadeInUpBig" data-wow-duration="1.3s" data-wow-delay="<%= 0.2 + (index * 0.2) %>s">
                        <div class="coffee_image">
                            <img src="<%= feature.image %>" alt="<%= feature.alt %>">
                        </div>
                        <div class="coffee_content">
                            <h4 class="coffee_title"><%= feature.title %></h4>
                            <p><%= feature.description %></p>
                        </div>
                    </div> <!-- single coffee -->
                </div>
                <% }); %>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>
    <!--====== COFFEE PART ENDS ======-->
    
    <!--====== ABOUT PART START ======-->
    <section id="about" class="about_area pt-120 pb-130">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section_title text-center pb-10">
