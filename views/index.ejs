    <!--====== COUNTER PART START ======-->
    <section id="counter" class="counter_area pt-50 pb-95 bg_cover text-center" style="background-image: url(/assets/images/counter_bg.png)">
        <div class="container">
            <div class="row">
                <% stats.forEach((stat, index) => { %>
                <div class="col-md-4 col-sm-12">
                    <div class="single_counter mt-30 wow fadeIn" data-wow-duration="1.3s" data-wow-delay="<%= 0.2 + (index * 0.3) %>s">
                        <span class="count"><span class="counter"><%= stat.number %></span></span>
                        <p><%= stat.label %></p>
                    </div> <!-- single counter -->
                </div>
                <% }); %>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>
    <!--====== COUNTER PART ENDS ======-->
    
    <!--====== GALLERY PART START ======-->
    <section id="gallery" class="gallery_area pt-120 pb-130">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section_title text-center pb-30">
                        <h4 class="title">Danh Mục Sản Phẩm</h4>
                        <span class="line">
                            <span class="box"></span>
                        </span>
                    </div> <!-- section title -->
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
        <div class="container">
            <div class="row gallery_active">
                <% categories.forEach((category) => { %>
                <div class="col-md-6 col-sm-12">
                    <div class="single_gallery mt-30">
                        <div class="gallery_image">
                            <img src="<%= category.image %>" alt="<%= category.alt %>">
                        </div>
                        <div class="gallery_content">
                            <h5 class="gallery_title"><%= category.title %></h5>
                            <p><%= category.description %></p>
                        </div>
                    </div> <!-- single gallery -->
                </div>
                <% }); %>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>
    <!--====== GALLERY PART ENDS ======-->
    
    <!--====== CUSTOMER PART START ======-->
    <section id="customer" class="customer_area pb-130">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section_title text-center pb-30">
                        <h4 class="title">Cảm Nhận Khách Hàng</h4>
                        <span class="line">
                            <span class="box"></span>
                        </span>
                        <p class="mt-3">Khách hàng nói gì về <%= site.name %>?</p>
                    </div> <!-- section title -->
                </div>
            </div> <!-- row -->
            <div class="row customer_active">
                <% testimonials.forEach((testimonial) => { %>
                <div class="col-lg-6">
                    <div class="single_customer d-sm-flex align-items-center mt-30">
                        <div class="customer_image">
                            <img src="<%= testimonial.image %>" alt="<%= testimonial.alt %>">
                        </div>
                        <div class="customer_content media-body">
                            <div class="customer_content_wrapper media-body">
                                <h5 class="author_name"><%= testimonial.name %></h5>
                                <span class="sub_title"><%= testimonial.location %></span>
                                <p>"<%= testimonial.content %>"</p>
                                <ul class="star">
                                    <% for(let i = 0; i < testimonial.rating; i++) { %>
                                    <li><i class="lni lni-star-filled"></i></li>
                                    <% } %>
                                </ul>
                            </div>
                        </div>
                    </div> <!-- single customer -->
                </div>
                <% }); %>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>
    <!--====== CUSTOMER PART ENDS ======-->
    
